{
    "blocks": [
      {
        "type": "prompt",
        "alignment": "left",
        "segments": [
          {
            "type": "session",
            "style": "powerline",
            "powerline_symbol": "\uE0B0",
            "foreground": "#ffffff",
            "background": "#c386f1",
            "leading_diamond": "",
            "trailing_diamond": "",
            "properties": {
              "display_host": false
            }
          },
          {
            "type": "path",
            "style": "powerline",
            "powerline_symbol": "",
            "foreground": "#ffffff",
            "background": "#ff479c",
            "properties": {
              "prefix": "  ",
              "home_icon": "~",
              "folder_icon": "\uF115",
              //"folder_separator_icon": "\uE0b1",
              "style": "agnoster"
            }
          },
          {
            "type": "git",
            "style": "powerline",
            "powerline_symbol": "\uE0B0",
            "foreground": "#193549",
            "background": "#fffb38",
             "properties": {
               "display_status": false,
               "display_status_detail": false,
               "display_stash_count": false,
               "display_upstream_icon": true,
               "status_colors_enabled": true,
               "local_changes_color": "#ff9248",
               "ahead_and_behind_color": "#f26d50",
               "behind_color": "#f17c37",
               "ahead_color": "#89d1dc",
               "stash_count_icon": "\uF692 "
             }
          },
        //   {
        //     "type": "node",
        //     "style": "powerline",
        //     "powerline_symbol": "",
        //     "foreground": "#ffffff",
        //     "background": "#6CA35E",
        //     "properties": {
        //       "prefix": " \uF898 ",
        //       "display_version": true
        //     }
        //   },
          {
            "type": "python",
            "style": "powerline",
            "powerline_symbol": "",
            "foreground": "#111111",
            "background": "#FFDE57",
            "properties": {
              "prefix": " \uE235 ",
              "display_version": true,
              "display_mode": "files",
              "display_virtual_env": true
            }
          },
          {
            "type": "dotnet",
            "style": "powerline",
            "powerline_symbol": "",
            "foreground": "#000000",
            "background": "#00ffff",
            "properties": {
              "prefix": " \uE77F "
            }
          },
          {
            "type": "root",
            "style": "powerline",
            "powerline_symbol": "",
            "foreground": "#111111",
            "background": "#ffff66",
            "properties": {
              "root_icon": ""
            }
          },
          {
            "type": "battery",
            "style": "powerline",
            "invert_powerline": false,
            "powerline_symbol": "\uE0B0",
            "foreground": "#ffffff",
            "background": "#f36943",
            "leading_diamond": "",
            "trailing_diamond": "",
            "properties": {
              "battery_icon": "\f583",
              "discharging_icon": " ",
              "charging_icon": " ",
              "charged_icon": " ",
              "color_background": true,
              "charged_color": "#4caf50",
              "charging_color": "#40c4ff",
              "discharging_color": "#ff5722",
              "postfix": " "
            }
          },
        //   {
        //     "type": "executiontime",
        //     "style": "plain",
        //     "foreground": "#ffffff",
        //     "background": "#83769c",
        //     "leading_diamond": "",
        //     "trailing_diamond": "",
        //     "properties": {
        //       "always_enabled": true,
        //       "prefix": "<transparent></> \ufa1e"
        //     }
        //   },
          {
            "type": "exit",
            "style": "powerline",
            "powerline_symbol": "\uE0B0",
            "foreground": "#ffffff",
            "background": "#2e9599",
            "leading_diamond": "",
            "trailing_diamond": "",
            "properties": {
              "display_exit_code": false,
              "always_enabled": true,
              "error_color": "#f1184c",
              "color_background": true,
              "prefix": " "
            }
          }
        ]
      }
    ],
    "final_space": true,
    "console_title": true,
    "console_title_style": "path"
  }
  
